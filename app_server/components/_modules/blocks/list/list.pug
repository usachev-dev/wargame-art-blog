mixin list(spec)
  - spec = spec || {}
  - list = spec.list || ['']
  - symbol = spec.symbol || ''

  .row.list
    .col-xs-12.col-sm-10.col-sm-offset-1.col-md-8.col-md-offset-2.col-lg-6.col-lg-offset-3
      ul
        each val, index in list
          if symbol==''
            li!= (index+1) + ') ' + val
          else
            li
              span(class="fa fa-#{symbol}")
              span!=' '+val
